<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="stock_transport_module_adding" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.stock</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="30"/>
            <!-- this field is indicate that this will inherit the res_settings_from_views from base. -->
            <field name="inherit_id" ref="base.res_config_settings_view_form" />
            <field name="arch" type="xml">
                <xpath expr="//block[@name='shipping_setting_container']" position="inside">
                    <setting id="module_stock_transport" help="Transport Management: organize packs in your fleet , or carriers.">
                        <field name="module_stock_transport" string="Dispatch Management System"/>
                    </setting>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
