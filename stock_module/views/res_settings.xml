<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="add_new_field_in_shipping_settings" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.stock</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="30"/>
            <field name="inherit_id" ref="base.res_config_settings_view_form" />
            <field name="arch" type="xml">
                <xpath expr="//block[@name='shipping_setting_container']" position="inside">
                    <setting id="module_fleet_module_add" help="the module is automatically installs whenever this will true">
                        <field name="module_fleet_module" string="Dispatch Management System"/>
                    </setting>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
